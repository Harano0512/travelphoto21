<div class="content_new-wrapper">
  <div class="content_new-container">
    <h2>新しい旅の記録を作成 <i class="fa fa-book" aria-hidden="true"></i></h2>
    <div class="content-form">
      <%= form_with(model: @content, local: true) do |f| %>
        <%= f.label :title,"タイトル" ,class:"content-title"%>
        <%= f.text_field :title ,placeholder:"例）沖縄満喫の旅", class:"content-form-box"%></br>
        <%= f.label :from_date,"出発日　"%>
        <%= f.date_field :from_date, class:"content-form-box" %></br>
        <%= f.label :return_date,"帰着日　"%>
        <%= f.date_field :return_date, class:"content-form-box" %></br>
        <%= f.submit "作成する" , class:"create-btn" %>
        <%= link_to "戻る",contents_path %>
      <% end %>
    </div>
  </div>
</div>