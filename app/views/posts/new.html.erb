<%= form_with(model: @post, url:content_posts_path, local: true) do |f| %>
  <%= f.label :subject, "タイトル" %>
  <%= f.text_field :subject, autofocus: true, autocomplete: "subject" %>
  <%= f.label :travel_date, "日付" %>
  <%= f.date_field :travel_date, autocomplete: "travel_date" %>
  <%= f.label :prefecture_id, "都道府県" %>
  <%= f.collection_select(:prefecture_id, Prefecture.all, :id, :name) %>
  <%= f.label :article, "テキスト" %>
  <%= f.text_area :article, autocomplete: "article" %>
  <%= f.label :images, "画像を選択" %>
  <%= f.file_field :images, multiple: true %>
  <%= f.submit "投稿する" %>
<% end %>